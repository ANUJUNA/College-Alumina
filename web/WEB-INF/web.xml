<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>test.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>YesNo</servlet-name>
        <servlet-class>test.YesNo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>test.ProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>XpDetailsServlet</servlet-name>
        <servlet-class>test.XpDetailsServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>test.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FinalRegServlet</servlet-name>
        <servlet-class>test.FinalRegServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FinalRegServlet</servlet-name>
        <url-pattern>/final</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegServlet1</servlet-name>
        <servlet-class>test.RegServlet1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegServlet1</servlet-name>
        <url-pattern>/reg1</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>home.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>YesNo</servlet-name>
        <servlet-class>test.YesNo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>YesNo</servlet-name>
        <url-pattern>/yesno</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>test.ProfileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>XpDetailsServlet</servlet-name>
        <servlet-class>test.XpDetailsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>XpDetailsServlet</servlet-name>
        <url-pattern>/xpdetails</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FilterServlet</servlet-name>
        <servlet-class>test.FilterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FilterServlet</servlet-name>
        <url-pattern>/filter</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TempServlet</servlet-name>
        <servlet-class>test.TempServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TempServlet</servlet-name>
        <url-pattern>/temp</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
